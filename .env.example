# ARC Solver Unified - Environment Configuration
# Copy this to .env and fill in your API keys

# App settings
APP_ENV=local
APP_HOST=0.0.0.0
APP_PORT=8000

# Provider API Keys (configure at least one)
# Recommended: GROQ for fastest inference
GROQ_API_KEY=

# Together AI (good for DeepSeek models)
TOGETHER_API_KEY=

# OpenRouter (access to many models)
OPENROUTER_API_KEY=

# Fireworks AI
FIREWORKS_API_KEY=

# OpenAI (for GPT models)
OPENAI_API_KEY=

# Anthropic (for Claude models)
ANTHROPIC_API_KEY=
